{
    "name": "Jimbot",
    "version": "0.2.9",
    "slug": "jimbot",
    "description": "Jim's Amazing Chatbot for Home Assistant",
    "arch": [
      "armhf",
      "armv7",
      "aarch64",
      "amd64",
      "i386"
    ],
    "startup": "application",
    "boot": "auto",
    "init": "false",
    "webui": "http://[HOST]:[PORT:8087]",
    "host_network": true,
    "options": {
      "openai_api_key": "Put your openai api key here"
    },
    "schema": {
      "openai_api_key": "str"
    },
    "ports": {
      "8080/tcp": 8087
    }
  }
  
